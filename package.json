{
  "name": "debug-challenge",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "install:all": "npm install && cd web && npm install",
    "server:dev": "NODE_ENV=development tsx watch server/index.ts",
    "server:test": "vitest run",
    "web:dev": "cd web && npm run dev",
    "dev": "concurrently \"npm run server:dev\" \"npm run web:dev\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "supertest": "^6.3.3",
    "concurrently": "^8.2.2"
  }
}

